{
    "name": "iman/ghafoori",
    "description": "Some description.",
    "keywords": ["framework", "package"],
    "license": "MIT",
    "type": "project",
    "require": {
        "hello/how": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imanghafoori\\LaravelMicroscope\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url" : "./a2"
        }
    ],
    "extra": {
        "some_key": {
            "dont-discover": [
                "*"
            ]
        }
    },
    "minimum-stability": "dev"
}
